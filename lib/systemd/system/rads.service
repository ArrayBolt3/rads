[Unit]
Description=Ram Adjusted Desktop Starter
Before=graphical.target
Before=getty.target

[Service]
Type=oneshot
ExecStart=/usr/lib/ram_adjusted_desktop_starter/ram_adjusted_desktop_starter
RemainAfterExit=yes
StandardOutput=tty
StandardInput=tty

[Install]
WantedBy=multi-user.target
